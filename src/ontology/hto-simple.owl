    <!-- https://w3id.org/pmd/ao/hto/AccelleratedCooling -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/AccelleratedCooling">
        <rdfs:comment xml:lang="en">Achieved via convection only.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Status of the cooling method active e.g., at the end of the nitriding process</skos:definition>
        <skos:prefLabel xml:lang="en">Accellerated Cooling</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/AcquisitionDate -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/AcquisitionDate">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">The point in time a Process Node was commissioned (e.g., the purchase date, first day of use), represented in IEEE 1003 &apos;Seconds since Epoch&apos;. </skos:definition>
        <skos:prefLabel xml:lang="en">Acquisition Date</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Annealing -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Annealing">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/HeattreatmentScope"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Heatteratment process with the scope to change the properties or microstructure of a sample or part. Typically no hardening is performed with this process.</skos:definition>
        <skos:prefLabel xml:lang="en">Annealing</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Atmosphere -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Atmosphere">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/Medium"/>
        <skos:definition>A surrounding influence or environment</skos:definition>
        <skos:prefLabel xml:lang="en">Atmosphere</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/AtmospherePressure -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/AtmospherePressure">
        <rdfs:comment xml:lang="en">Effective atmosphere preasure in chamber to keep process save (must be 2-3 mbar above ambient prssure)</rdfs:comment>
        <rdfs:comment xml:lang="en">Parameter usually slightly higher than ambient pressure for open process to environment to prevent/reduce diffusion of oxygen into the furnace, e.g. by leckage, to avoid explosive or dangerous atmopheres, could be bar, mbar or Pa</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Parameter describing the intended atmosphere pressure inside a chamber. </skos:definition>
        <skos:prefLabel xml:lang="en">Atmosphere Pressure</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/BelowAmbientPressure -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/BelowAmbientPressure">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">status of atmosphere: reduced pressure below ambient pressure </skos:definition>
        <skos:prefLabel xml:lang="en">Below Ambient Pressure</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/CarbonPotential -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/CarbonPotential">
        <rdfs:comment xml:lang="de">Nitrocarburieren mit Kohlenstoffträger, aus Messwerten abgeleitet KC</rdfs:comment>
        <rdfs:comment xml:lang="en">See standard DIN 1310</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Carbon content of a specimen of pure iron in equilibrium with a carbourizing medium under specific conditions.</skos:definition>
        <skos:prefLabel xml:lang="en">Carbon Potential</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Carburizing -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Carburizing">
        <rdfs:comment xml:lang="de">Potentiell ableitbar mit bestimmten Fluss-Schwellenwerten</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Heattreatment process with the scope to transfer carbon from the atmosphere to the component surface.</skos:definition>
        <skos:definition xml:lang="de">Prozessschritt, während dessen Kohlenstoff aus der Atmosphäre an der Bauteiloberfläche übertragen wird</skos:definition>
        <skos:prefLabel xml:lang="de">Aufkohlen</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Carburizing</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Chamber -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Chamber">
        <rdfs:comment xml:lang="de">unklare Zuordnung innerhalb der &quot;spec. dep. continuants&quot;. Eine Kammer existiert nur als Teil eines Gerätes (Device), könnte aber selbst noch einmal als independent continuant definiert werden, da sich in der Kammer Chargen, Gestelle, Thermoelemente etc. befinden.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Systemboundary of a distinct Heattreatment Process. Typically includes media (e.g., Helium or Vaccum).</skos:definition>
        <skos:prefLabel xml:lang="en">Chamber</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/ChamberPressure -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/ChamberPressure">
        <rdfs:comment xml:lang="en">measured atmophere pressure in retort, measured using the vacuum sensor </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">designated pressure in retort during evacuation phase pre to nitriding process or during low pressure nitriding process </skos:definition>
        <skos:prefLabel xml:lang="en">Chamber Pressure</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/ChamberScope -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/ChamberScope">
        <rdfs:comment xml:lang="de">Sollte mit Unit-Volume (m^3) verbunden sein</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Functional furnace chamber.</skos:definition>
        <skos:definition xml:lang="de">Funktionale Ofenkammer.</skos:definition>
        <skos:definition xml:lang="en">Position of the batch in a multi-chamber furnace
A specialization of hasProcessNodeComponent representing distinguishable heat treatment chambers of a furnaces.
can be: heating chamber / cooling chamber</skos:definition>
        <skos:prefLabel xml:lang="en">ChamberScope</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Wärmebehandlungsofenkammer</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/CollectionInterfaceHint -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/CollectionInterfaceHint">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">A label in support of identifying a network interface and/or address used by a process node to provide primary data stored in ontologies </skos:definition>
        <skos:prefLabel xml:lang="en">Collection Interface Hint</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/ConcentrationH2 -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/ConcentrationH2">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/FurnaceAtmosphere"/>
        <rdfs:comment xml:lang="en">Ammonia is usually intenionally induced;</rdfs:comment>
        <rdfs:comment xml:lang="de">Einzelne Werte durch auf das Molekül spezifizierte Messsonden.</rdfs:comment>
        <rdfs:comment>Hydrogen can be formed during decomposition of e.g. ammonia, acetylene or can be intenionally induced e.g. in forming gas.</rdfs:comment>
        <rdfs:comment xml:lang="en">Might be calculated from model or measurement.</rdfs:comment>
        <rdfs:comment xml:lang="en">Oxygen concentration in process gas composition, oxygen can be formed during decomposition of e.g. carbon monoxide or dioxide or can be intenionally induced e.g. as air, synthetic air or water or enters the furnace via leckage.</rdfs:comment>
        <rdfs:comment xml:lang="en">Water concentration in process gas composition, if nominal water is intenionally induced; units could be e.g. mol/m³ or °C; signal could be in mA or mV or in vol.-% or °C (dew point)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Concentration in process gas composition; units could be e.g. mol/m³; signal could be in mA or mV or in vol.-%</skos:definition>
        <skos:prefLabel xml:lang="en">Concentration</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Controlled -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Controlled">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">This Scope expresses that a :ValueObject is actively controlled during the process, e.g. a heating process where the temperature change is done in controlled intervals until the desired temperature is reached while an uncontrolled heating process would reach the desired temperature as fast as the furnace allows it to.</skos:definition>
        <skos:prefLabel xml:lang="en">Controlled</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Convection -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Convection">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">konvection during heating, heating in atmosphere, not in vacuum </skos:definition>
        <skos:prefLabel xml:lang="en">Convection</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/CoolingFlag -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/CoolingFlag">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">represents whether a sequence (of a process recipe) is for cooling to nominal temperature or not, takes value 0 or 1 </skos:definition>
        <skos:prefLabel xml:lang="en">Cooling Flag</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/CoolingProcess -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/CoolingProcess">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">A process in which the temperature is lowered from a higher to a lower level.</skos:definition>
        <skos:definition xml:lang="de">Ein Prozess, bei dem die Temperatur von einem höherem auf ein niedrigeres Niveau abgesenkt wird.</skos:definition>
        <skos:definition xml:lang="en">process description during cooling process, e.g. fast cooling, free furnace cooling, controled cooling</skos:definition>
        <skos:prefLabel xml:lang="de">Abkühlprozess</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Cooling Process</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/DewPoint -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/DewPoint">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">dew point of the atmosphere, measured during process or calculated from lambda / oxygen probe signal </skos:definition>
        <skos:prefLabel xml:lang="en">Dew Point</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/DurationTillTargetTemperature -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/DurationTillTargetTemperature">
        <rdfs:comment xml:lang="de">Erwärmdauer = Heizzeit + Durchwärmdauer</rdfs:comment>
        <rdfs:comment xml:lang="de">Maß für die tatsächliche Dauer eines Erwärmvorgangs.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Measurement representing the actual duration of a heating procedure.</skos:definition>
        <skos:prefLabel xml:lang="en">Duration Till Target Temperature</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Erwärmdauer</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/DurationToReachTargetTemperature -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/DurationToReachTargetTemperature">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Parameter representing a derived duration of a heat up segment (in hours, minutes or seconds) </skos:definition>
        <skos:prefLabel xml:lang="en">Duration To Reach Target Temperature</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Fan -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Fan"/>
    


    <!-- https://w3id.org/pmd/ao/hto/FanStateFast -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FanStateFast">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/Rotation"/>
        <rdfs:label xml:lang="en">Fan State Fast</rdfs:label>
        <skos:definition xml:lang="en">Boolean parameter representing a fast fan speed setting in contrast to the default speed setting.</skos:definition>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FastCooling -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FastCooling">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/HeattreatmentMethod"/>
        <rdfs:comment xml:lang="de">Anlagenbedingt.</rdfs:comment>
        <rdfs:comment xml:lang="de">Schnellkühlung kann z.B. durch erhöhten Kühlwasserdurchfluss erfolgen.
Auch erhöhte Gaskühlung durch ventilation.</rdfs:comment>
        <skos:definition xml:lang="de">Schnellkühlung</skos:definition>
        <skos:definition xml:lang="de">Stark beschleunigte Abkühlung ohne Abschreckmedium.</skos:definition>
        <skos:prefLabel xml:lang="en">Fast Cooling</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FineVacuum -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FineVacuum">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">status of the atmosphere, evacuation step without nitriding potential </skos:definition>
        <skos:prefLabel xml:lang="en">Fine Vacuum</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FlowRate -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FlowRate">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/MediaMovement"/>
        <rdfs:comment xml:lang="en">Effective flow rate might be generated by the system with a manual input in Percent.</rdfs:comment>
        <rdfs:comment>Use Scope.Medium and pmdoc:relatesTo chemical element from chebi; units could be l/min, l/h, m³/h, ...</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Parameter representing the intended induced flow rate of a medium, e.g. Oil, Gases, Fluids in general.</skos:definition>
        <skos:example>ex:flowrateH2 a :FlowRate ;
                       co:relatesTo chebie:Hydrogen ;
                      co:unit qudt:Mol-per-litre .</skos:example>
        <skos:prefLabel xml:lang="en">Flow Rate</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FlowRateTotal -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FlowRateTotal">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/MediaMovement"/>
        <rdfs:comment>Units could be l/min, l/h, m³/h, ... </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">Measured value for the total flow rate of all process media</skos:definition>
        <skos:prefLabel xml:lang="en">Flow Rate Total</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/Fluid -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/Fluid">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/Medium"/>
        <skos:definition>A substance (such as a liquid or gas) tending to flow or conform to the outline of its container</skos:definition>
        <skos:prefLabel xml:lang="en">Fluid</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FurnaceAtmosphere -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FurnaceAtmosphere">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">atmosphere type present in retort (vacuum, nitriding, neutral, carburizing)</skos:definition>
        <skos:prefLabel xml:lang="en">Furnace Atmosphere</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/FurnaceCooling -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/FurnaceCooling">
        <rdfs:subClassOf rdf:resource="https://w3id.org/pmd/ao/hto/HeattreatmentMethod"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/hto/"/>
        <skos:definition xml:lang="en">free cooling with deactivated heating unit and no additonal cooling by e.g. agitade nitrogene -&gt; furnace cool </skos:definition>
        <skos:prefLabel xml:lang="en">Furnace Cooling</skos:prefLabel>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/ao/hto/GasIngressFrontState -->

    <owl:Class rdf:about="https://w3id.org/pmd/ao/hto/GasIngressFrontState">
        <rdfs:label xml:lang="en">Gas Ingress Front State</rdfs:label>
        <skos:definition xml:lang="en">Boolean parameter that opens a magnetic-controlled valve to induce frontsited gas ingress (if set to true) into the heating chamber. Choice of ingress can have an effect on the effective gas flow direction into or through the batch and can impact in homogeneties </skos:definition>
    </owl:Class>
    


